{% block sw_cms_block_text_teaser_section %}
<div class="sw-cms-block-text-teaser-section sw-cms-block-text-teaser-section--three-col">
    <div class="sw-cms-block-text-teaser-section__teaser">
        <slot name="left"></slot>
    </div>

    <div class="sw-cms-block-text-teaser-section__content">
        <slot name="right"></slot>
    </div>

    <div class="sw-cms-block-text-teaser-section__image">
        <img v-if="imageUrl" :src="imageUrl" alt="">
    </div>
</div>
{% endblock %}
