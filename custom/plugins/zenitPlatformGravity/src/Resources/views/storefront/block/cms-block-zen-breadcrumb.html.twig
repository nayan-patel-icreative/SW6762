{% block block_zen_breadcrumb %}
    {% set element = block.slots.getSlot('content') %}

    <div class="col-12">
        <div class="zen-breadcrumb" data-cms-element-id="{{ element.id }}">
            {% block block_zen_breadcrumb_inner %}
                {% sw_include '@Storefront/storefront/element/cms-element-' ~ element.type ~ '.html.twig' ignore missing %}
            {% endblock %}
        </div>
    </div>

{% endblock %}
